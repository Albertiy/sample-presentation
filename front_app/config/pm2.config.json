{
  "apps": [
    {
      "name": "yilabaoFrontApp",
      "cwd": "./",
      "exec_interpreter": "node",
      "script": "./runScript.js",
      "watch": "./",
      "ignore_watch": ["node_modules", ".next", "log", "*.log"],
      "max_restarts": 1,
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "./log/pm2_error.log",
      "out_file": "./log/pm2_out.log",
      "pid_file": "./log/pm2_pid.log"
    }
  ]
}
